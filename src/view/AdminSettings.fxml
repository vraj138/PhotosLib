<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminSettingsController">
   <children>
      <Label layoutX="10.0" layoutY="15.0" prefHeight="40.0" prefWidth="165.0" text="Admin Settings">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="adminDelete" layoutX="182.0" layoutY="535.0" mnemonicParsing="false" onAction="#onDeleteUser" prefHeight="40.0" prefWidth="110.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Delete user" textFill="WHITE" AnchorPane.leftAnchor="180.0" AnchorPane.rightAnchor="45.0">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="adminAdd" layoutX="45.0" layoutY="535.0" mnemonicParsing="false" onAction="#onAddUser" prefHeight="40.0" prefWidth="110.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Add User" textFill="WHITE" AnchorPane.leftAnchor="45.0" AnchorPane.rightAnchor="180.0">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="adminLogOut" layoutX="261.0" layoutY="23.0" mnemonicParsing="false" onAction="#onLogOutBtnClicked" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Log Out" textFill="WHITE">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <ListView fx:id="listView" layoutX="16.0" layoutY="60.0" prefHeight="464.0" prefWidth="301.0" />
   </children>
</AnchorPane>
