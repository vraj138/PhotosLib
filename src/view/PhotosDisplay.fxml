<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PhotosDisplayController">
   <children>
      <Label fx:id="albumNameLabel" contentDisplay="TOP" layoutX="85.0" layoutY="12.0" prefHeight="32.0" prefWidth="136.0" text="Label">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="addPhotosBtn" layoutX="20.0" layoutY="537.0" mnemonicParsing="false" onAction="#onAddPhotosBtn" prefHeight="33.0" prefWidth="93.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Add Photos" textFill="WHITE" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="222.0">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="removePhotosBtn" layoutX="132.0" layoutY="537.0" mnemonicParsing="false" onAction="#onRemovePhotosBtn" prefHeight="33.0" prefWidth="93.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Remove Photos" textFill="WHITE" AnchorPane.leftAnchor="132.0" AnchorPane.rightAnchor="110.0">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="backBtn" layoutX="13.0" layoutY="17.0" mnemonicParsing="false" onAction="#onBackBtnClicked" prefHeight="19.0" prefWidth="52.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Back" textFill="WHITE">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="logOutBtn" layoutX="257.0" layoutY="17.0" mnemonicParsing="false" onAction="#onLogOutBtnClicked" prefHeight="22.0" prefWidth="64.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Log Out" textFill="WHITE">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <ListView fx:id="photoListview" layoutX="22.0" layoutY="58.0" onMouseClicked="#onMouseClicked" prefHeight="464.0" prefWidth="199.0" />
      <TextField fx:id="tfCopyAlbum" layoutX="232.0" layoutY="308.0" onAction="#onCopyPhotoBtn" prefHeight="25.0" prefWidth="93.0" promptText="Enter Album" />
      <TextField fx:id="tfMoveAlbum" layoutX="232.0" layoutY="458.0" onAction="#onMovePhotoBtn" prefHeight="25.0" prefWidth="93.0" promptText="Enter Album" />
      <Label layoutX="233.0" layoutY="288.0" text="Copy To:" />
      <Label layoutX="233.0" layoutY="440.0" text="Move To:" />
      <Button fx:id="copyPhotoBtn" layoutX="237.0" layoutY="352.0" mnemonicParsing="false" onAction="#onCopyPhotoBtn" prefHeight="25.0" prefWidth="84.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Copy Photo" textFill="WHITE">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="movePhotoBtn" layoutX="236.0" layoutY="505.0" mnemonicParsing="false" onAction="#onMovePhotoBtn" prefHeight="25.0" prefWidth="84.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Move Photo" textFill="WHITE">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
