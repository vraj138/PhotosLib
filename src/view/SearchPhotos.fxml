      </Button>
      <Button fx:id="searchByDateBtn" layoutX="210.0" layoutY="508.0" mnemonicParsing="false" onAction="#onSearchByTagsBtn" prefHeight="34.0" prefWidth="111.0" style="-fx-background-color: #000000#000000; -fx-background-radius: 10;" text="Search by Date" textFill="WHITE">
